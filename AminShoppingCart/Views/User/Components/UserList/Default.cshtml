@{
    ViewData["Title"] = "View";
    Layout = null;
}
@model List<AminShoppingCart.Models.ViewModels.UserViewModel>
<div class="card-body">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">User Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.UserName</td>
                 
                </tr>
            }
        </tbody>
    </table>
</div>